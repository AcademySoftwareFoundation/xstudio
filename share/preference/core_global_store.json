{
	"core": {
		"global_store":{
			"autosave_interval": {
				"path": "/core/global_store/autosave_interval",
				"default_value": 60,
				"description": "Interval at which we autosave in seconds.",
				"value": 60,
				"minimum": 10,
				"maximum": 600,
				"datatype": "int",
				"context": ["APPLICATION"]
			},
			"autosave_enable": {
				"path": "/core/global_store/autosave_enable",
				"default_value": true,
				"description": "Enable autosave of preferences.",
				"value": true,
				"datatype": "bool",
				"context": ["APPLICATION"]
			}
		}
	}
}