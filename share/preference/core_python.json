{
	"core": {
		"python":{
			"enabled": {
				"path": "/core/python/enabled",
				"default_value": true,
				"description": "Enable embedded python.",
				"value": true,
				"datatype": "bool",
				"context": ["APPLICATION"]
			},

			"snippets":{
				"path": {
					"path": "/core/python/snippets/path",
					"default_value": ["${XSTUDIO_ROOT}/snippets","${HOME}/xStudio/snippets"],
					"description": "Paths to snippet dirs",
					"value": null,
					"datatype": "json",
					"context": ["APPLICATION"]
				}
			}
		}
	}
}